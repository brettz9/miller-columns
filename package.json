{
  "name": "miller-columns",
  "version": "0.6.0",
  "description": "Miller Columns in ES6 Modules JavaScript",
  "main": "src/index.js",
  "scripts": {
    "rollup": "rollup -c",
    "eslint": "eslint .",
    "demo-no-open": "pkill -f \".bin/static -p\" ; npm run eslint && npm run rollup && npm run start",
    "demo": "npm run eslint && npm run rollup && opn http://localhost:8092/demos/ && npm run start",
    "start": "static -p 8092",
    "test": "npm run eslint && npm run rollup && testcafe chrome tests/**/*.js",
    "watch": "chokidar '**/*.js' -c 'npm run demo-no-open' -i 'dist' -i 'node_modules' & npm run demo-no-open",
    "dontuse-unfinished-watch": "chokidar '**/*.js' -c 'pkill -f \".bin/static -p\" ; npm run eslint && npm run rollup && reload -b -d \"demos\"' -i 'dist'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/miller-columns.git"
  },
  "keywords": [
    "miller-columns",
    "columns",
    "column",
    "browser"
  ],
  "author": "Brett Zamir",
  "contributors": [
    {
      "name": "White Magic Software",
      "url": "http://jsfiddle.net/yckart/cbtnemc7/"
    }
  ],
  "engines": {},
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brettz9/miller-columns/issues"
  },
  "homepage": "https://github.com/brettz9/miller-columns#readme",
  "dependencies": {},
  "devDependencies": {
    "axe-testcafe": "^1.1.0",
    "babel-core": "6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chokidar-cli": "^1.2.1",
    "eslint": "5.6.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "jquery": "^3.3.1",
    "load-stylesheets": "^0.7.0",
    "node-static": "0.7.10",
    "opn-cli": "^3.1.0",
    "reload": "^2.3.1",
    "rollup": "0.65.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^2.0.2",
    "testcafe": "^0.22.0"
  }
}
